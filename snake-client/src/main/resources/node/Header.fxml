<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="headerMainAnchorPane" prefHeight="50.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane fx:id="headerScrollPane" prefHeight="60.0" prefWidth="800.0">
         <content>
            <HBox fx:id="headerHBox" alignment="CENTER" prefHeight="50.0" prefWidth="798.0" style="-fx-spacing: 10;">
               <children>
                  <Button fx:id="headerRunButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="220.0" text="Uruchom">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
                  <Label text="IP :" />
                  <TextField fx:id="headerIpTextField" />
                  <Label text="Ping :" />
                  <TextField fx:id="headerPingTextField" editable="false" prefHeight="25.0" prefWidth="50.0" />
                  <Label text="FPS :" />
                  <TextField fx:id="headerFpsTextField" editable="false" prefHeight="25.0" prefWidth="50.0" />
               </children>
            </HBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
