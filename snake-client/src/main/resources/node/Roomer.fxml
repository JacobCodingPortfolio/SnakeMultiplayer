<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane fx:id="infoPane" fitToHeight="true" fitToWidth="true" layoutX="300.0" prefHeight="400.0" prefWidth="300.0">
         <content>
            <Button fx:id="startGame" mnemonicParsing="false" text="Start" />
         </content>
      </ScrollPane>
      <TableView fx:id="gameUsers" prefHeight="200.0" prefWidth="300.0" />
      <TableView fx:id="gameTables" layoutY="200.0" prefHeight="200.0" prefWidth="300.0" />
   </children>
</AnchorPane>
